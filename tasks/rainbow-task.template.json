{
  "family": "awesomeapp-rainbow",
  "executionRoleArn": "arn:aws:iam::696665953192:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "rainbow",
      "image": "696665953192.dkr.ecr.eu-west-1.amazonaws.com/awesomeapp/rainbow",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}